<h1><%= @employee.name %></h1>
<% if @employee.events.any? %>
    <table>
      <tr>
        <th>Event</th>
        <th>Time</th>
      </tr>
      <td>
        <%= render @events %>
      </td>
    </table>
 <% end %>
<%= link_to "create new Event", root_url %><br/>
<%= calendar @attendancs do |attendanc| %>
<%= link_to attendance.employee_id, @attendance  %>
<% end %>
<%= link_to "attendance", attendanc_path %>



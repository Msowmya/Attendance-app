<%= form_for(@event) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <p>
      <%= f.label :event_name %>
      <%= f.text_field :event_name %>
    </p>
    <p>
      <%= f.label :Time %>
      <%= f.text_field :start_time %>
    </p>
    <%= f.submit "Post" %>
<% end %>